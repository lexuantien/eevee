{
  "name": "@eevee/react-storybook-addon",
  "version": "1.0.0",
  "private": true,
  "description": "eevee react storybook addon",
  "main": "lib-commonjs/index.js",
  "module": "lib/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/lexuantien/eevee"
  },
  "license": "MIT",
  "scripts": {
    "start": "yarn storybook",
    "test": "jest --passWithNoTests",
    "storybook": "node ../../../scripts/storybook/runner",
    "type-check": "tsc -b tsconfig.json"
  },
  "devDependencies": {
    "@eevee/eslint-plugin": "*",
    "@eevee/scripts": "*"
  },
  "dependencies": {
    "@eevee/react-theme": "*",
    "@eevee/react-provider": "*",
    "tslib": "^2.1.0"
  },
  "peerDependencies": {
    "@types/react": ">=16.8.0",
    "@types/react-dom": ">=16.8.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@storybook/addons": "^6.4.19",
    "@storybook/api": "^6.4.19",
    "@storybook/components": "^6.4.19",
    "@storybook/core-events": "^6.4.19",
    "@storybook/theming": "^6.4.19"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib-commonjs/index.js"
    }
  }
}
