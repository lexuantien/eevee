{
  "name": "@eevee/react-mdx-tool",
  "version": "1.0.0",
  "description": "MDX tool",
  "main": "lib-commonjs/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/lexuantien/eevee"
  },
  "license": "MIT",
  "scripts": {
    "clean": "just-scripts clean",
    "code-style": "just-scripts code-style",
    "just": "just-scripts",
    "lint": "just-scripts lint",
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006",
    "test": "jest --passWithNoTests",
    "type-check": "tsc -b tsconfig.json"
  },
  "devDependencies": {
    "@eevee/eslint-plugin": "*",
    "@eevee/scripts": "*",
    "@types/node": "^17.0.36"
  },
  "dependencies": {
    "@eevee/react-theme": "*",
    "@griffel/react": "1.1.0",
    "tslib": "2.4.0",
    "@eevee/react-utilities": "*",
    "@eevee/react-icons": "*",
    "@eevee/react-mdx-comp": "*",
    "mdx-bundler": "9.0.1",
    "esbuild": "^0.14.39",
    "remark-mdx-images": "1.0.3"
  },
  "peerDependencies": {
    "@types/react": ">=16.8.0",
    "@types/react-dom": ">=16.8.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib-commonjs/index.js"
    }
  }
}
